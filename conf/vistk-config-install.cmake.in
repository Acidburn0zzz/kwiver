# Include the library targets
include("${CMAKE_CURRENT_LIST_DIR}/vistk-config-targets.cmake")

set(VISTK_VERSION
  "@vistk_version@")

set(VISTK_BUILT_SHARED
  "@BUILD_SHARED_LIBS@")

# vistk include directory
set(VISTK_INCLUDE_DIR
  "@CMAKE_INSTALL_PREFIX@/include")
if (WIN32)
  set(VISTK_LIBRARY_DIR
    "@CMAKE_INSTALL_PREFIX@/bin")
else ()
  set(VISTK_LIBRARY_DIR
    "@CMAKE_INSTALL_PREFIX@/lib@LIB_SUFFIX@")
endif ()
set(VISTK_MODULE_DIR
  "${VISTK_LIBRARY_DIR}/vistk")
set(VISTK_DOCUMENTATION_DIR
  "@CMAKE_INSTALL_PREFIX@/share/doc/vistk-@vistk_version@")

set(VISTK_PIPE_INSTALL_PATH
  "@CMAKE_INSTALL_PREFIX@/share/vistk")
set(VISTK_PIPE_INCLUDE_INSTALL_PATH
  "${VISTK_PIPE_INSTALL_PATH}/include")

set(VISTK_ENABLE_PYTHON
  "@VISTK_ENABLE_PYTHON@")
set(VISTK_ENABLE_PYTHON3
  "@VISTK_ENABLE_PYTHON3@")
set(VISTK_PYTHON_EXECUTABLE
  "@PYTHON_EXECUTABLE@")
