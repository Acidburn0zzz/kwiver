!include _warp3.pipe

process _warp4
  :: warp_image
  :pixtype[ro]{CONF} warp:pixtype
  :pixfmt[ro]{CONF} warp:pixfmt

connect from _warp_dist.dist/image/4
        to   _warp4.image
connect from _warp_dist.dist/transform/4
        to   _warp4.transform

connect from _warp4.warped_image
        to   _warp_coll.coll/image/4
