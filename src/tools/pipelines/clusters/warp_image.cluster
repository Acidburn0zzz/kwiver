cluster warp
  :# Warp an image
  :: warp
  :# The pixtype of the images to warp.
  :pixtype byte
  :# The pixfmt of the images to warp.
  :pixfmt  rgb
  :# The image to warp.
  imap from image
       to   warp1.image
  :# The warped image.
  omap from warp1.warped_image
       to   warped

process warp1
  :: warp_image
  :pixtype[ro]{CONF} warp:pixtype
  :pixfmt[ro]{CONF}  warp:pixfmt
